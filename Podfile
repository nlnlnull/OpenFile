  use_frameworks!
  pod 'SnapKit'
  pod 'MASShortcut'
  pod 'LoginServiceKit', :git => 'https://github.com/Clipy/LoginServiceKit.git'
  pod 'Sparkle'
  # Pods for OpenFile

end
